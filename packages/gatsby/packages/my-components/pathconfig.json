{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": { 
      "~/site-config": ["./src/site-config"]
    }
  },
  "babel": {
    "root": ".",
    "alias": {
      "~/site-config": "./src/site-config"
    }
  },
  "jest": {
    "^~/site-config$": "<rootDir>/src/site-config"
  },
  "webpack": {
    "~/site-config": "/site-config"
  }
}
