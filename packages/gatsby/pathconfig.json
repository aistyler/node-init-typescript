{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": { 
      "@/src/*": ["./src/*"],
      "@/libs/*": ["./src/libs/*"],
      "@/components": ["./src/components"],
      "@/component-views/*": ["./src/components/views/*"],
      "@/page-views/*": ["./src/page-views/*"],
      "@/pages/*": ["./src/pages/*"],
      "@/styles/*": ["./src/styles/*"]
    }
  },
  "babel": {
    "root": ".",
    "alias": {
      "@/src": "./src",
      "@/libs": "./src/libs",
      "@/components": "./src/components",
      "@/component-views": "./src/components/views",
      "@/page-views": "./src/page-views",
      "@/pages": "./src/pages",
      "@/styles": "./src/styles"
    }
  },
  "jest": {
    "^@/(.*)$": "<rootDir>/src/$1"
  },
  "webpack": {
    "@/src": "/src",
    "@/libs": "/src/libs",
    "@/components": "/src/components",
    "@/component-views": "/src/components/views",
    "@/page-views": "/src/page-views",
    "@/pages": "/src/pages",
    "@/styles": "/src/styles"
  }
}
